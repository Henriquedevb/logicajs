//escreva um algoritmo para imprimir na tela os números de 1 a 5
algoritmo "semnome"
var
   num: inteiro
inicio
      num <-1000
      repita
             num <- num + 1
             escreval(num)
      ate(num > 4)
fimalgoritmo
//GARANTE que o bloco interno será executado PELO MENOS uma vez
